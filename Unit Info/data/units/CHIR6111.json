    "unit_learning_outcomes": [
      {
        "description": "Demonstrate a basic knowledge of the functional anatomy of the human body including: the biomechanical effects of an adjustment or mobilisation and the indications for their use; structural analysis as it relates to posture and dysfunction; and motion palpation as it relates to spinal and peripheral joints.",
        "code": "ULO6"
      },
      {
        "description": "Demonstrate a thorough knowledge of human neuroanatomy.",
        "code": "ULO5"
      },
      {
        "description": "Perform basic static and motion palpation on all spinal and peripheral joints in the body.",
        "code": "ULO3"
      },
      {
        "description": "Demonstrate an understanding of the basic tenants underpinning modern scientific research",
        "code": "ULO7"
      },
      {
        "description": "Demonstrate an understanding of peripheral and spinal joint mechanics.",
        "code": "ULO4"
      },
      {
        "description": "Perform a basic set of spinal and a full set of peripheral adjustments and/or mobilisations with a level of psychomotor skill that is appropriate for these procedures i.e. tactile/palpatory skills and hand/body/eye co-ordination of practitioner movements.",
        "code": "ULO1"
      },
      {
        "description": "Control these procedures with regard to patient position, practitioner position, primary contact, secondary contact, lock-up/set-up, speed, amplitude and line of drive.",
        "code": "ULO2"
      }
    ],
