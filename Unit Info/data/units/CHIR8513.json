    "unit_learning_outcomes": [
      {
        "description": "Describe, perform, and interpret physical and functional capacity assessments for the musculoskeletal system, and relate impairments or findings to presenting musculoskeletal conditions.",
        "code": "ULO2"
      },
      {
        "description": "Demonstrate patient-centred communication, education, and coaching during patient assessment, management, and to aid behaviour change.",
        "code": "ULO5"
      },
      {
        "description": "Identify risks and barriers to behaviour change with respect to exercise participation for the prevention and management of common musculoskeletal conditions.",
        "code": "ULO3"
      },
      {
        "description": "Develop person-centered, goal-oriented exercise programs to target a variety of musculoskeletal conditions within an evidenced-based framework, with consideration of biopsychosocial and lifestyle factors, and incorporation of passive care where appropriate.",
        "code": "ULO4"
      },
      {
        "description": "Apply key principles relating to exercise science and physical activity in relation to body functions, health, well-being, physical performance, and the prevention and management of disability.",
        "code": "ULO1"
      }
    ],
